{
  "name": "aviris-components",
  "version": "0.1.0",
  "description": "A collection of customized shadcn/ui components",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aviris": "./dist/cli/bin/aviris.js"
  },
  "files": [
    "dist",
    "cli",
    "README.md"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "tsup",
    "build:components": "tsup",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepublishOnly": "npm run build:components"
  },
  "tsup": {
    "entry": [
      "components/custom/index.ts",
      "cli/bin/aviris.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": false,
    "clean": true,
    "external": [
      "react",
      "react-dom"
    ],
    "target": "node16",
    "platform": "node",
    "noExternal": [
      "aviris-components"
    ],
    "tsconfig": "tsconfig.build.json"
  },
  "peerDependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "commander": "^11.1.0",
    "framer-motion": "^12.3.1",
    "fs-extra": "^11.3.0",
    "lucide-react": "^0.474.0",
    "next-themes": "^0.4.4",
    "react-icons": "^5.4.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.17",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.19.0",
    "eslint-config-next": "14.1.0",
    "next": "^14.1.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.3.6",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "shadcn",
    "tailwindcss"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/aviris"
  }
}
